{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "cs-aws-test",
      "host":  {
        "sourcePath": "/var/app/current/circleci-practice"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "cs-aws-test",
      "image": "276282166840.dkr.ecr.us-east-1.amazonaws.com/circleci-practice:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "cs-aws-test",
          "containerPath": "/usr/src/app"
        }
      ]
    }
  ]
}